{
  "name": "k-redux-factory",
  "version": "6.0.1",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "license": "MIT",
  "repository": "https://github.com/alakarteio/k-redux-factory",
  "bugs": "https://github.com/alakarteio/k-redux-factory/issues",
  "scripts": {
    "lint:js": "eslint --ext js,jsx src",
    "lint": "npm-run-all --parallel lint:*",
    "cmd:build": "microbundle --external all --sourcemap false -f es,cjs -i ",
    "build:core": "yarn cmd:build src/index.js",
    "build:helper": "yarn cmd:build src/helpers.js -o helpers/index.js",
    "build": "npm-run-all --parallel build:*",
    "test": "jest",
    "coveralls": "jest --coverage --collectCoverageFrom=src/**/*.{js,jsx} && cat ./coverage/lcov.info | coveralls",
    "ci": "npm-run-all --parallel coveralls lint"
  },
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "7.2.0",
    "@babel/plugin-external-helpers": "7.2.0",
    "@babel/preset-env": "7.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-external-helpers": "6.22.0",
    "coveralls": "3.0.2",
    "cross-env": "5.2.0",
    "eslint": "5.10.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-import-resolver-node": "0.3.2",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "jest": "23.6.0",
    "jest-environment-jsdom": "23.4.0",
    "microbundle": "0.8.3",
    "npm-run-all": "4.1.5"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb"
    ],
    "plugins": [
      "babel"
    ],
    "globals": {
      "fetch": false
    },
    "env": {
      "browser": true
    },
    "rules": {
      "object-curly-newline": "off",
      "semi": [
        2,
        "never"
      ],
      "max-len": [
        2,
        {
          "code": 200
        }
      ]
    }
  }
}
